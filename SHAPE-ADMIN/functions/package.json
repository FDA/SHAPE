        "request": "^2.88.2",
        "twilio": "^3.66.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/nodemailer": "^6.4.4",
        "@types/request": "^2.48.8",
        "@typescript-eslint/eslint-plugin": "^5.22.0",
        "@typescript-eslint/parser": "^5.22.0",
        "eslint": "^8.14.0",
        "eslint-plugin-import": "^2.26.0",
        "firebase-functions-test": "^0.2.0",
        "typescript": "^4.5.5"
