{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ],
    "source": "functions"
  },
  "firestore": {
   "rules": "functions/rules/firestore.rules"
  },
  "storage": {
     "rules": "functions/rules/storage.rules"
  },
  "emulators": {
    "firestore": {
      "port": 8082
    },
    "functions": {
      "port": 5001
    },
    "storage": {
       "port": 9199
    }
  }
}
